    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.556.0",
    "ulid": "^2.3.0"
  },
  "peerDependencies": {
    "@medusajs/framework": "^0.0.2"
  },
  "keywords": [
    "medusa-plugin",
    "medusa-plugin-s3"
